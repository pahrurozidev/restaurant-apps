{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "indent": [
            "error",
            4
        ],
        "no-underscore-dangle": 0,
        "no-useless-return": 0,
        "no-plusplus": 0,
        "no-restricted-globals": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-useless-path-segments": 0,
        "no-undef": 0,
        "prefer-destructuring": 0,
        "consistent-return": 0,
        "eqeqeq": 0,
        "no-prototype-builtins": 0,
        "no-return-assign": 0,
        "import/prefer-default-export": 0
    }
}